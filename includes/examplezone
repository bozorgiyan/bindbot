@ IN SOA ns1.domainv. web.domainv. (
                    2010062801 ; Serial
                    10800 ; Refresh
                    3600 ; Retry
                    604800 ; Expire
                    86400 ; Minimum

)
domainv. IN NS ns1.domainv.
domainv. IN  NS  ns2.domainv.
domainv.       IN A ipv
ns1.domainv.   IN A ipv
ns2.domainv.   IN A ipv
www.domainv.   IN A ipv
ftp.domainv.   IN A ipv
mail.domainv.  IN A ipv
domainv.       IN MX 10 mail.domainv.
